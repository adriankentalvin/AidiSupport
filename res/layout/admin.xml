<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:orientation="vertical"
  android:layout_width="match_parent"
  android:layout_height="match_parent"
  android:background="@drawable/background">

	<Button 
		android:layout_marginTop="40dp"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:text="create Doctors profile"
		android:id="@+id/btnAdminDoctors"/>
	<Button 
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:text="Diseases"
		android:id="@+id/btnAdminDiseases"/>
	<Button 
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:text=" create Medicines profile"
		android:id="@+id/btnAdminMedicines"/>
	<Button 
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:text="create symptoms for disease "
		android:id="@+id/btnAdminSymptoms"/>
	<Button
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:text="create Hospitals for doctors"
		android:id="@+id/btnAdminHospitals"/>
	<Button
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:text="create specialty for doctors"
		android:id="@+id/btnAdminSpecialtyDoctors"/>
		
</LinearLayout>
